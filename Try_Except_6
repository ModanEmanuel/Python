def inversare_lista(numbers):
    try:
        for number in numbers:
            int(number)
    except ValueError:
        return "Lista conÈ›ine elemente invalide"
    return numbers[::-1]

print(inversare_lista([1, 2, 3, 4]))
print(inversare_lista([]))
print(inversare_lista(["unu", "doi", 3, 4]))
